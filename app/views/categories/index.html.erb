<h1>Categories#index</h1>
<p>Find me in app/views/categories/index.html.erb</p>
<%= link_to 'New Category', new_category_path %>
<br>
<% @categories.each do |category| %>
<%= link_to category.category_name, category_items_path(category.id) %>
<%= link_to 'Edit', edit_category_path(category.id) %>
<%= link_to 'Delete', category_path(category.id), method: :delete %>
<br>
<% end %>
